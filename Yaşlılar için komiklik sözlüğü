meme_dict = {
            "CRINGE": "Garip ya da utandırıcı bir şey",
            "LOL": "Komik bir şeye verilen cevap",
            "EZ":"Kolaydı",
            "GLHF":"İyi şanslar iyi oyunlar.",
            "GG":"İyi oyundu.",
            "LMAO":"Delice gülmek",
            "AFK":"Bilgisayar başında değilim",
            "EKMEK REİS":"Oturan yerde yatan ekmek yiyen köpek"
            }
word=input("Anlamadığınız bir kelime yazın(Hepsini büyük harfle yazın!)")

if word in meme_dict.keys():
    print(meme_dict[word])    
else:
    print("Kelime eşleşmedi.")
